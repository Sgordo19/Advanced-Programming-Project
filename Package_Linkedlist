package Project;

public class Package_Linkedlist 
{
	static class SinglyLinkedList 
	{ 
	        
	        private PackageNode.Node Head;         
	        
	        public SinglyLinkedList() 
	        { 
	            Head = null; 
	        } 	         
	      
	        public SinglyLinkedList(Project.PackageNode.Node h)  
	        { 
	            Head = h; 
	        } 	         
	      
	        public Project.PackageNode.Node GetHead() 
	        { 
	            return Head; 
	        } 	         	         
	      
	        public void SetHead(Project.PackageNode.Node head) 
	        { 
	            Head = head; 
	        } 
	 
	        public void InsertAtFront(Package data) 
	        { 
	        	PackageNode.Node temp = new PackageNode.Node();
	        	
	            if(temp != null)  
	            { 
	              
					temp.setData(data); 

	                temp.setNextNode(null); 
	                 
	                if(Head == null)  
	                { 
	                    Head = temp; 
	                } 
	                else 
	                { 
	                    temp.setNextNode(Head); 
	                    Head = temp;  
	                } 
	            } 
	            else  
	            { 
	                System.out.println("Error! List is full (Out of Memory), can NOT add a new node");
	            } 
	        }
	         
	        public void InsertAtBack(Package dataToInsert) 
	        { 
	            PackageNode.Node temp1, temp2;
	            temp1 = new PackageNode.Node();  
	            if(temp1 !=  null) 
	            { 
	                temp1.setData(dataToInsert);  
	                temp1.setNextNode(null);  
	                 
	                if(Head == null)  
	                { 
	                    Head = temp1;  
	                } 
	                else  
	                { 
	                    temp2 = Head;  
	                    while(temp2.getNextNode() != null)  
	                    { 
	                        temp2 = temp2.getNextNode();  
	                    } 
	                    temp2.setNextNode(temp1);  
		                } 
	                 
	            } 
	            else  
	            { 
	                System.err.println("Error! List is full (Out of Memory), can NOT add a new node"); 
	            } 
	        } 
	         
	         
	        public void InsertAtBack(int package_id, int p_weight,int p_quantity,String destination,boolean p_status, int package_type)
	    	  
	        { 
	            PackageNode.Node temp1 = new PackageNode.Node(package_id, p_weight,p_quantity,destination,p_status, package_type); 

	            if(temp1 != null) 
	            { 
	                if(Head == null)  
	                { 
	                    Head = temp1;  

	                } 
	                else  
	                { 
	                    PackageNode.Node temp2 = Head;  
	                    while(temp2.getNextNode() !=  null)  
	                    { 
	                        temp2 = temp2.getNextNode();  
	                    } 
	                    temp2.setNextNode(temp1);  
	 
	                } 
	            } 
	            else 
	            { 
	               System.err.println("Error! List is full (Out of Memory), can NOT add a new node"); 
	            } 
	        } 
	         
	         
	        public int CountNodes() 
	        { 
	            int count = 0;
	            PackageNode.Node curr = Head;
	            while(curr != null)  
	            { 
	                count++;  
	                curr = curr.getNextNode();  
	            } 
	            return count;  
	        } 
	         
	         
	        public boolean SearchForANode(int packageId) 
	        { 
	            boolean isFound = false; 
	             
	            PackageNode.Node curr = Head;   
	            while (curr != null)  
	            { 
	                if(curr.getData().getPackage_id() == packageId) {
	                    isFound = true; 
	                    break;  
	                } 
	                curr = curr.getNextNode();
	            } 
	         
	            return isFound; 
	        } 
	 
	 
	        public void DisplayList() 
	        { 
	            PackageNode.Node curr = Head; 
	            
	            while (curr != null)  
	            { 
	                System.out.print("["+ curr.getData() + "]"); 
	                curr = curr.getNextNode();  
	            } 
	            
	            //System.out.println("NULL"); // test  
	        } 
	         
	         
	        public boolean IsEmpty() 

	        { 
	            if(Head == null)  
	            { 
	                return true;  
	            } 
	            return false;  
	        } 
	         
	        public boolean IsFull() 
	        { 
	            Node temp = new Node(); 
	            if(temp != null)  
	            { 
	                temp = null;  
	                return false; 
	            } 
	            return true; 
	        } 
	         
	         
	         
	        public Package DeleteANode(int PackageID) 
	        { 
	             
	            Package dataToReturn = new Package(); 
	             
	            if(!IsEmpty()) 
	            { 
	                PackageNode.Node curr = Head, prev=null;   
	                 
	                while(curr != null)  
	                { 
	                    if(curr.getData().getPackage_id() == PackageID) 
	                    { 
	                        if(curr == Head) 
	                        { 
	                            Head = Head.getNextNode();  
	                        } 
	                        else 
	                        { 
	                            prev.setNextNode(curr.getNextNode()); 
	                        } 
	                        dataToReturn = curr.getData();  
	                        curr = null; 
	                        break;  
	                    } 
	                    prev = curr; 
	                    curr = curr.getNextNode(); 
	                } 	             
	            } 
	            else 
	            { 
	                System.err.println("The list is empty; there is nothing to delete!"); 
	            } 	             
	            return dataToReturn; 
	        } 
		} 

}
